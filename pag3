def flow_control(k):

    s=''

    if (k==0):
        s = "Varible k = %d equals 0." % k
    elif (k==1):
        s = "Varible k = %d equals 1." % k
    else:
        s = "Varible k = %d does not equal 0 or 1. " % k


    print(s)

def main():
        i = 0
        j = 1
        k = 2

        flow_control(i)
        flow_control(j)
        flow_control(k)

if __name__ == "__main__": 
    main()